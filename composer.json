{
    "name": "jdferreira/autocrud",
    "description": "A Laravel CRUD generator that creates model, controllers, views, factories, form requests for validation, routes and tests",
    "authors": [
        {
            "name": "Jo√£o D. Ferreira",
            "email": "jotomicron@gmail.com",
            "homepage": "http://jdferreira.net"
        }
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.1",
        "phpunit/phpunit": "^7.1",
        "laravel/framework": "^5.6",
        "doctrine/dbal": "^2.9",
        "friendsofphp/php-cs-fixer": "^2.16",
        "matt-allan/laravel-code-style": "^0.3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8",
        "phpstan/phpstan": "^0.12.39"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Ferreira\\AutoCrud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "App\\": "vendor/orchestra/testbench-core/laravel/app"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ferreira\\AutoCrud\\ServiceProvider"
            ]
        }
    }
}
